<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="android.com.GraphTask" >

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name="android.com.GraphTask.ActivitySelectGraphProblem"
            android:label="@string/title_activity_select_graph_problem" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="android.com.GraphTask.ActivityInputQuantityNodes"
            android:label="@string/title_activity_activity_input_quantity_nodes">
        </activity>
        <activity
            android:name="android.com.GraphTask.ActivityInputAdjacentNodes"
            android:label="@string/title_activity_activity_input_adjacent_nodes" >
        </activity>
        <activity
            android:name="android.com.GraphTask.ActivityResultDFSandBFS"
            android:label="@string/title_activity_dfsand_bfs" >
        </activity>
        <activity
            android:name="android.com.GraphTask.ActivityInputWeightEdges"
            android:label="@string/title_activity_input_weight_edges" >
        </activity>
        <activity
            android:name="android.com.GraphTask.ActivityResultKruskal"
            android:label="@string/title_activity_result" >
        </activity>
    </application>

</manifest>
